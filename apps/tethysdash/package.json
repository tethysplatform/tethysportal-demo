{
  "name": "tethysdash_frontend",
  "version": "0.0.1",
  "description": "Frontend React project for tethysdash",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config ./reactapp/config/webpack.config.js",
    "build": "webpack --mode production --config ./reactapp/config/webpack.config.js",
    "lint": "npx eslint reactapp",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "test": "jest --silent --coverage",
    "cleartestCache": "jest --clearCache",
    "test:watch": "npm run test -- --watch",
    "jest-preview": "jest-preview yarn test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tiptap/extension-color": "^2.12.0",
    "@tiptap/extension-font-family": "^2.12.0",
    "@tiptap/extension-highlight": "^2.12.0",
    "@tiptap/extension-history": "^2.12.0",
    "@tiptap/extension-image": "^2.12.0",
    "@tiptap/extension-list-keymap": "^2.12.0",
    "@tiptap/extension-subscript": "^2.12.0",
    "@tiptap/extension-superscript": "^2.12.0",
    "@tiptap/extension-task-item": "^2.12.0",
    "@tiptap/extension-task-list": "^2.12.0",
    "@tiptap/extension-text-align": "^2.12.0",
    "@tiptap/extension-typography": "^2.12.0",
    "@tiptap/extension-underline": "^2.12.0",
    "@tiptap/pm": "^2.12.0",
    "@tiptap/react": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "css-loader": "^6.5.1",
    "dompurify": "^3.1.6",
    "dotenv": "^16.0.1",
    "file-loader": "^6.2.0",
    "html-react-parser": "^5.1.18",
    "html2canvas": "^1.4.1",
    "ol": "10.4.0",
    "ol-mapbox-style": "12.4.0",
    "plotly.js-strict-dist-min": "^2.35.2",
    "prismjs": "^1.28.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.2",
    "react-color-palette": "",
    "react-confirm": "^0.3.0-7",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.0",
    "react-icons": "^5.3.0",
    "react-joyride": "^2.9.3",
    "react-plotly.js": "^2.5.1",
    "react-resize-detector": "^11.0.1",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.8.0",
    "react-simple-wysiwyg": "^3.1.1",
    "sass": "^1.49.0",
    "sass-loader": "^12.3.0",
    "simple-xml-to-json": "^1.2.3",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "swiper": "^11.2.1",
    "uuid": "^8.3.2",
    "webpack": "^5.64.4",
    "webpack-dev-server": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-private-property-in-object": "^7.18.2",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-prismjs": "^2.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "browserslist": "^4.20.4",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.16.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-fixed-jsdom": "^0.0.9",
    "jest-location-mock": "^1.0.9",
    "jest-preview": "^0.3.1",
    "jest-watch-typeahead": "^1.1.0",
    "msw": "^1.3.3",
    "prettier": "^3.4.2",
    "react-app-polyfill": "^3.0.0",
    "react-select-event": "^5.5.1",
    "react-test-renderer": "^18.1.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime"
    ],
    "ignorePatterns": [
      "**/transforms/*.js",
      "**/config/*"
    ],
    "globals": {
      "__webpack_init_sharing__": true,
      "__webpack_share_scopes__": true
    },
    "rules": {
      "react/no-unescaped-entities": 0
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/reactapp"
    ],
    "collectCoverageFrom": [
      "reactapp/**/*.{js,jsx,ts,tsx}",
      "!reactapp/**/*.d.ts",
      "!reactapp/__tests__/**/*"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/reactapp/__tests__/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/reactapp/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/reactapp/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/reactapp/__tests__/utilities/*",
      "<rootDir>/reactapp/__tests__/transforms/*",
      "<rootDir>/reactapp/__tests__/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/reactapp/__tests__/transforms/babelTransform.js",
      "^.+\\.css$": "<rootDir>/reactapp/__tests__/transforms/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/reactapp/__tests__/transforms/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(swiper|ol|ol-mapbox-style|plotly.js|react-plotly.js|color-(space|parse|rgba|name)|rbush|quickselect|quick-lru|pbf|earcut|react-color-palette|uuid)[/\\\\]).+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePaths": [
      "<rootDir>/reactapp",
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactapp/__tests__/utilities/fileMock.js",
      "^.+\\.module\\.(css|sass|scss|less)$": "identity-obj-proxy",
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "babel-plugin-styled-components",
      [
        "prismjs",
        {
          "languages": "all",
          "theme": "tomorrow",
          "css": true
        }
      ]
    ]
  }
}
